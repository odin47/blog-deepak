{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "react/react-in-jsx-scope": "off"
    },
    "globals": {
        "React": "writable"
    }
}
