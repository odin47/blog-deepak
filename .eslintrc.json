{
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends": ["airbnb-base", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"react/react-in-jsx-scope": "off",
		"import/extensions": "off",
		"import/no-unresolved": "off",
		"indent": [2, "tab"],
		"no-tabs": 0,
		"object-curly-newline": [
			"error",
			{
				"ImportDeclaration": "never"
			}
		]
	},
	"globals": {
		"React": "writable"
	}
}
